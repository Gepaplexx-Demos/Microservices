{
  "build": {
    "workspace-size": "2Gi",
    "language": {
      "name": "java",
      "version": "openjdk-11",
      "type": {
        "name": "maven",
        "version": "3.8.3",
        "configuration": {
          "goals": "clean install",
          "options": ""
        }
      },
      "configuration": {
      }
    },
    "static-code-analysis": {
      "active": "true",
      "type": "sonarqube",
      "configuration": {
      }
    }
  },
  "image": {
    "registry": "ghcr.io",
    "repository": "demo-microservice",
    "containerfile": "Containerfile"
  },
  "integrationtest": {
    "active": "active",
    "type": "newman",
    "configuration": {
      "port": "8080",
      "collection": "postman.json",
      "collection-path": "postman",
      "env-var": ""
    }
  },
  "vulnerability-scan": {
    "active": "true",
    "type": "trivy",
    "configuration": {
      "fail": {
        "active": "true",
        "severity": "CRITICAL"
      },
      "extra-args": ""
    }
  }
}